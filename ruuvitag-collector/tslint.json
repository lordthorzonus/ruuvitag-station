{
  "extends": "tslint:recommended",
  "rules": {
    "interface-name": false,
    "quotemark": {
      "options": "single"
    },
    "object-literal-sort-keys": false,
    "no-console": false,
    "no-bitwise": false,
    "no-empty-interface": false,
    "arrow-parens": false,
    "no-var-requires": false,
    "no-reference": false
  }
}
